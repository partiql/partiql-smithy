type position struct {
    x: int32,
    y: int32,
    z: int32,
};

type decimal struct {
    coefficient: int32,
    exponent: int32,
};

type coordinates struct {
    lat: decimal,
    lon: decimal,
};

type array_fixed int32[20];
type array_variable int32[];

type order enum { ASC, DESC };

type singleton unit;

